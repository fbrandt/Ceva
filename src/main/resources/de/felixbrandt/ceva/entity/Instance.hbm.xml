<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"><hibernate-mapping>
	<class name="de.felixbrandt.ceva.entity.Instance">
		<id name="instance">
			<generator class="increment"></generator>
		</id>
		<property name="name" />
		<property name="checksum" />
		<property name="content" type="org.hibernate.type.TextType" />
		<property name="active" />
		<property name="timelimit" />
	</class>
</hibernate-mapping>